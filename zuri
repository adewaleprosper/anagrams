# Check if two words are anagrams 
# Example:
# find_anagrams("hello", "check") --> False
# find_anagrams("below", "elbow") --> True

w1 = "Listen"
w2 = "silence" 

def find_anagram(w1, w2):
    # [assignment] Add your code here
    if(sorted(w1) == sorted(w2)):
        print(w1 + " and " + w2 + " are anagrams.")
        return True
    else:
        return False
print(find_anagram)
